generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model finance_price {
  id        String   @id @default(cuid())
  brand     String   
  title     String
  gen       String
  year      Int
  price     Int
  group     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
